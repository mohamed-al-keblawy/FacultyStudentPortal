@model IEnumerable<dynamic>

<h2>University Weekly Schedule</h2>

<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>Course</th>
            <th>Faculty</th>
            <th>Room</th>
            <th>Type</th>
            <th>Day</th>
            <th>Start</th>
            <th>End</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var s in Model)
        {
            <tr>
                <td>@s.Course</td>
                <td>@s.Faculty</td>
                <td>@s.Room</td>
                <td>@s.SectionType</td>
                <td>@s.DayOfWeek</td>
                <td>@s.StartTime</td>
                <td>@s.EndTime</td>
            </tr>
        }
    </tbody>
</table>
